// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model LinkedinPost {
  id         String   @id @default(cuid())
  title      String
  created_at DateTime @default(now())
  postData   Json?
  userId     String
  postResult String?
}
